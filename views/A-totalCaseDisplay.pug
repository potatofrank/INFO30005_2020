extends A-layout

block content
    .main_content3
        a(href='admin')
            button Back
        br
        br
        each val in totalCases
            table.center
                tr
                    th Name
                    th Value
                tr
                    td id
                    td #{val._id}
                tr
                    td Date
                    td #{val.Date}
                tr
                    td Accumulate Confirmed Cases
                    td #{val.Accumulate_Confirmed_Cases}
                tr
                    td Current Existing Cases
                    td #{val.Current_Existing_Cases}
                tr
                    td Accumulate Deaths
                    td #{val.Accumulate_Deaths}
                tr
                    td Accumulate Cured_Cases
                    td #{val.Accumulate_Cured_Cases}
            br
            br
